<!-- Dialog Address -->
<div class="modal fade dialogbox" id="dialog-address" data-backdrop="static" tabindex="-1" role="dialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-icon text-success">
        <ion-icon name="map-outline"></ion-icon>
      </div>
      <div class="modal-header">
        <h5 class="modal-title">Abrir Mapa</h5>
      </div>
      <div class="modal-body">
        <%= humanize_address address %>
      </div>
      <div class="modal-footer">
        <div class="btn-list">
          <a href="<%= google_maps_link(humanize_address address) %>" target="_system" class="btn btn-text-primary btn-block">Abrir no Mapa</a>
          <a href="javascript:;" class="btn btn-text-secondary btn-block" data-dismiss="modal">Fechar</a>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- * Dialog Address -->